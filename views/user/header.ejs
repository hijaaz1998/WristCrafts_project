        .pagination a.active {
        background-color: #007bff; /* Highlighted background color */
        color: #fff; /* Font color for the active page */
        border: 1px solid #007bff; /* Optional border for the active page */
        }
        /* CSS */
        .discount-row {
        display: none;
        }

        .custom-dropdown {
            position: relative;
            width: 100%;
        }
        
        .coupon-details {
            display: none;
            position: absolute;
            top: 100%;
            left: 0;
            width: 100%;
            background-color: #fff;
            border: 1px solid #ccc;
            padding: 10px;
            z-index: 100;
        }
        
        .custom-dropdown select:focus + .coupon-details {
            display: block;
        }
        </style>
<style>
        /* Pagination container styles */
ul.pagination {
  display: flex;
  list-style: none;
  padding: 0;
  margin: 20px 0;
}

/* Pagination link styles */
ul.pagination li {
  margin-right: 10px;
}

ul.pagination a {
  text-decoration: none;
  background-color: #f2f2f2;
  color: #333;
  padding: 5px 10px;
  border-radius: 5px;
  transition: background-color 0.3s;
}

ul.pagination a:hover {
  background-color: #333;
