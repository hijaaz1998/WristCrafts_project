        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    
                </div>
                
            </div>
            <div class="card mb-4">
                
                <div class="card-body">
                    <div class="table-responsive" style="max-height: 400px; overflow-y: scroll;">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Payment Method</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (let i = 0; i < orders.length; i++) { %>
                                <tr>
                                    <td><%= orders[i]._id %></td>
                                    <td><b><%= orders[i].user.fname + " " + orders[i].user.lname  %></b></td>
                                    <td><%= orders[i].user.email %></td>
                                    <td><%= orders[i].total %></td>
                                    <td><%= orders[i].paymentMethod %></td>
                                    <td>
                                        <% if (orders[i].orderStatus === 'Accepted') { %>
                                          <span class="badge rounded-pill alert-success">Accepted</span>
                                        <% } else if (orders[i].orderStatus === 'Returned') { %>
                                          <span class="badge rounded-pill alert-danger">Returned</span>
                                        <% } else if (orders[i].orderStatus === 'Pending') { %>
                                          <span class="badge rounded-pill alert-warning">Pending</span>
                                        <% } else if (orders[i].orderStatus === 'Cancelled') { %>
                                          <span class="badge rounded-pill alert-info">Cancelled</span>
                                        <% } else if (orders[i].orderStatus === 'Packed') { %>
                                          <span class="badge rounded-pill alert-primary">Packed</span>
                                        <% } else if (orders[i].orderStatus === 'Shipped') { %>
                                          <span class="badge rounded-pill alert-primary">Shipped</span>
                                        <% } else if (orders[i].orderStatus === 'Delivered') { %>
                                          <span class="badge rounded-pill alert-success">Delivered</span>
                                        <% } else { %>
                                          <span class="badge rounded-pill alert-secondary">Unknown</span>
                                        <% } %>
                                      </td>
                                      
                                    
                                    <td><%= orders[i].createdOn.toDateString() %></td>
                                    <td class="text-end">
                                        <a href="/admin/viewDetails?orderId=<%= orders[i]._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                        <div class="dropdown">
                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <% if (orders[i].orderStatus !== 'Cancelled' && orders[i].orderStatus !== 'Delivered'  && orders[i].orderStatus !== 'Returned') { %>
                                              <div class="dropdown-menu">
                                                <a class="dropdown-item text-danger" href="/admin/orderPacked/<%= orders[i]._id %>">Packed</a>
                                                <a class="dropdown-item text-danger" href="/admin/orderShipped/<%= orders[i]._id %>">Shipped</a>
                                                <a class="dropdown-item text-danger" href="/admin/orderDelivered/<%= orders[i]._id %>">Delivered</a>
                                              </div>
                                            <% } %>
                                            
                                            
                                            
                                        </div> <!-- dropdown //end -->
                                    </td>
                                </tr>
                                <% } %>
                            </tbody>
                        </table>
                        
                    </div> <!-- table-responsive //end -->
                    <ul class="pagination">
                        <ul class="pagination">
                            <% const maxPageLinks = 3; %>
                            <% const startPage = Math.max(1, currentPage - Math.floor(maxPageLinks / 2)); %>
                            <% const endPage = Math.min(totalPages, startPage + maxPageLinks - 1); %>
                            
                            <% if (currentPage > 1) { %>
                              <li>
                                <a href="/admin/orders?page=1">First</a>
                              </li>
                            <% } %>
                          
                            <% if (startPage > 1) { %>
                              <li>
                                <span class="ellipsis">...</span>
                              </li>
                            <% } %>
                          
                            <% for (let i = startPage; i <= endPage; i++) { %>
                              <li>
                                <a href="/admin/orders?page=<%= i %>" class="<%= i === currentPage ? 'active' : '' %>">
                                  <%= i %>
                                </a>
                              </li>
                            <% } %>
                          
                            <% if (endPage < totalPages) { %>
                              <li>
                                <span class="ellipsis">...</span>
                              </li>
                            <% } %>
                          
                            <% if (currentPage < totalPages) { %>
                              <li>
                                <a href="/admin/orders?page=<%= totalPages %>">Last</a>
                              </li>
                            <% } %>
                          </ul>
                          
                    </ul>
